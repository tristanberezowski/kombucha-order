<%= simple_form_for [ :admin, container ] do |f| %>
    <section class="form-inputs">
    <%= f.input :name %>
    <%= f.input :volume, label: "Volume (mL)" %>
    <%= f.input :environmental_fee, label: "Environmental Fee" %>
  </section>

  <section class="form-links">
    <%= f.submit %>
  </section>
<% end %>