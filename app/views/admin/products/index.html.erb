<section class="admin-products-index">
  <h1>Products</h1>

  <% @products.each do |product|%>
    <div><%= product.name %></div>
    <div><%= product.price %></div>
    <a href="/admin/products/edit/<%= product.id %>" >Edit</a>
  <% end %>

  <%= link_to(
    'New Liquid Product',
    new_admin_product_path(
      product: {
        selectable_type: 'LiquidSelection'
      }
    )
  ) %>
</section>
